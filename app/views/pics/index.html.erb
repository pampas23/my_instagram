<div class="col-md-12">
  <div class="transitions-enabled" id="pics">
    <% @pics.each do |pic| %>
      <div class="box panel panel-default">
        <%= link_to (image_tag pic.image.url), pic %>
        <h2>
          <%= link_to pic.tittle, pic %>
        </h2>
      </div>
    <% end %>
  </div>
</div>
